{
	"AWS[EBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "vinothr90/multi-client",
			"hostname": "client",
			"essential": "false"
		},
		{
			"name": "server",
			"image": "vinothr90/multi-server",
			"hostname": "api",
			"essential": "false"
		},
		{
			"name": "worker",
			"image": "vinothr90/multi-worker",
			"hostname": "worker",
			"essential": "false"
		},
		{
			"name": "nginx",
			"image": "vinothr90/multi-nginx",
			"hostname": "nginx",
			"essential": "true",
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": ["client","server"]			
		}
	]
}